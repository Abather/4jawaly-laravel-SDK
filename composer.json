{
    "name": "sms4jawaly/laravel-sdk",
    "description": "SMS4Jawaly SDK for Laravel applications",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jawaly",
            "email": "support@4jawaly.com"
        }
